<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base application theme. -->
    <style name="Theme.InteractivityDemo" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dolby_blue</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorPrimaryVariant">@color/dolby_light_blue</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dolby_purple</item>
        <item name="colorSecondaryVariant">@color/dolby_pink</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>
        <item name="android:navigationBarColor">@color/black</item>
        <!-- Customize your theme here. -->
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>
    <style name="SplashTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>
        <item name="android:windowBackground">@drawable/logo</item>
        <item name="android:navigationBarColor">@color/black</item>
    </style>
</resources>